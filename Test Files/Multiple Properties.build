<project default="main">
	<target name="main" description="main">
		<tstamp property="tmp" pattern="yyyyMMdd" verbose="true" unless="${property::exists('tmp')}" />
		<!-- unless="${property::exists('tmp')}" -->
		<echo message="${tmp}" />
		<call target="Not Main" />
	</target>
	
	<target name="Not Main" description="not main">
		<tstamp property="tmp" pattern="yyyyMMdd" verbose="true" unless="${property::exists('tmp')}" />		
		<echo message="${tmp}" />		
	</target>
</project>
